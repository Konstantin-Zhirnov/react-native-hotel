{
  "expo": {
    "name": "Hotel",
    "slug": "react-native-hotel",
    "privacy": "public",
    "platforms": ["ios","android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-icon.png",    
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hotelloftgarden.rooms",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.hotelloftgarden.rooms",
      "versionCode": 1,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
